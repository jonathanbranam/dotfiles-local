[init]
  defaultBranch = master
[push]
  followTags = true
[pull]
  ff = only
[alias]
  fixup = -c sequence.editor=: rebase -i --autosquash
[user]
  name = Jonathan Branam
  email = jonathan.branam@gmail.com
[core]
  excludesfile = /Users/jbranam/.gitignore
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
